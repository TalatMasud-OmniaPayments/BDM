<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

	<Color x:Key="background">#FF232829</Color>
	<SolidColorBrush x:Key="backgroundBrush" Color="{StaticResource background}"/>
	<SolidColorBrush x:Key="foreground" Color="#D5EAE9"/>
	<SolidColorBrush x:Key="foreground.dark" Color="{StaticResource background}"/>
	<SolidColorBrush x:Key="background.header" Color="#FFBCE4E2"/>

	<SolidColorBrush x:Key="off" Color="#FF2E4246"/>
	<SolidColorBrush x:Key="on" Color="#FF19852D"/>
	<SolidColorBrush x:Key="error" Color="Red"/>
	<SolidColorBrush x:Key="alarm" Color="#FFF1602A"/>
	<SolidColorBrush x:Key="selected" Color="#FF637576"/>
	<SolidColorBrush x:Key="selected.listItem" Color="#FF0ACBE2"/>
	<SolidColorBrush x:Key="hover" Color="#FF212C2C"/>
	<SolidColorBrush x:Key="button.enabled" Color="#FFCDDA49"/>
	<SolidColorBrush x:Key="button.disabled" Color="#FF5C5A5A"/>
	<SolidColorBrush x:Key="button.hover" Color="#FFB1ECF0"/>
	<SolidColorBrush x:Key="enter" Color="#FFCDDA49"/>
	<SolidColorBrush x:Key="enter.edit" Color="White"/>
</ResourceDictionary>